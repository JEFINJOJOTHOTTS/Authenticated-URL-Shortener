<head>
    <meta charset="utf-8">
    <title>Evara - eCommerce HTML Template</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <!-- <link rel="shortcut icon" type="image/x-icon" href="assets/imgs/theme/favicon.svg"> -->
    <!-- Template CSS -->
    <link rel="stylesheet" href="css/main.css">
</head>

<body>
   
    <main class="main">
        <section class="pt-70 pb-150">
            <div class="row pl-100">
                <h2 style="color:#046963;">URL Shortener</h2>
            </div>
        
            <div class="pt-50 container">
                
                <div class="row">
                    <div class="col-lg-10 m-auto">
                        <div class="row">
                            <div class="col-lg-6 m-auto">
                                <div class="login_wrap widget-taber-content p-30 background-white border-radius-5">
                                    <div class="padding_eight_all bg-white">
                                        <div class="heading_s1">
                                            <h3 class="mb-30">Create an Account</h3>
                                        </div>
                                        <form method="post"  id="signUpForm" onsubmit="return signUpValidateForm()" action="/signup">
                                            <div class="form-group">
                                                <input type="text" required="" id="mail" onkeyup="emailValidation()"
                                                    name="mail" placeholder="Email">
                                            </div>
                                            <div class="form-group">
                                                <input required="" id="password" type="password" onkeyup="passwordValidation()"
                                                    name="password" placeholder="Password">
                                                <div class="login_footer form-group">
                                                    <div class="chek-form">
                                                        <div class="custome-checkbox">
                                                            <input class="form-check-input" type="checkbox"
                                                                onclick="showPassword()" name="checkbox"
                                                                id="exampleCheckbox1" value="">
                                                            <label class="form-check-label"
                                                                for="exampleCheckbox1"><span>Show
                                                                    Password</span></label>

                                                            <div id="message">
                                                                <span id="messagePasword" style="color: rgb(126, 5, 5);" class="invalid">
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="form-group">
                                                <input required="" id="confirmPassword" type="password" onkeyup="confirmPasswordValidation()"
                                                    name="password" placeholder="Confirm password">
                                                <div class="login_footer form-group">
                                                    <div class="chek-form">
                                                        <div class="custome-checkbox">
                                                            <input class="form-check-input" type="checkbox"
                                                                onclick="showConfirmPassword()" name="checkbox"
                                                                id="exampleCheckbox2" value="">
                                                            <label class="form-check-label"
                                                                for="exampleCheckbox2"><span>Show Confirm
                                                                    Password</span></label>

                                                            <div id="message">
                                                                <span id='confirmMessage' style="color: rgb(126, 5, 5);" class="invalid">
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                            <% if (message){%>
                                                <div class="row pt-3 justify-content-center text-center">
                                                    <p class=" mb-0" style="color: rgb(143, 49, 49);">
                                                        <%=message%>
                                                    </p>
                                                </div>
                                                <%}%>
                        
                                            <div class="form-group">
                                                <button type="submit" class="btn btn-fill-out btn-block hover-up"
                                                    value="Submit">Submit &amp; Register</button>
                                            </div>
                                        </form>
                                        <div class="row">
                                            <p>Have an account : <a href="/login">Login </a></p>
                                        </div>
                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="text-center">
                    <h5 class="mb-5">Now Loading</h5>
                    <div class="loader">
                        <div class="bar bar1"></div>
                        <div class="bar bar2"></div>
                        <div class="bar bar3"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Vendor JS-->
    <script src="js/vendor/jquery-3.6.0.min.js"></script>
    <!-- Template  JS -->
    <script src="js/main.js"></script>
    <script src="js/validation.js"></script>


</body>


